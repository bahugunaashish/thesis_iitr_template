file = {:D$\backslash$:/Documents/mendeley/Sharma, Hazarika/Geotechnical and Geological Engineering/Sharma, Hazarika - 2013 - Assessment of Liquefaction Potential of Guwahati City A Case Study.pdf:pdf},
issn = {09603182},
journal = {Geotech. Geol. Eng.},
keywords = {Case study,Cyclic stress ratio,Liquefaction potential,Standard penetration test},
number = {5},
pages = {1437--1452},
title = {{Assessment of Liquefaction Potential of Guwahati City: A Case Study}},
volume = {31},
year = {2013}
