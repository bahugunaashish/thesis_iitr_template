@article{Fu2008,
abstract = {We constrain anistropic seismic structure in the southernmost Tibet and the Tethyan Himalays using SKS and SKKS phases recorded from a temporary seismic network, operated from July 2004 to August 2005 in conjunction with the Hi-Climb project. Shear wave splitting is not detected at 16 stations, most of which are located near and to the north of the Indus-Yalong suture (M). For the first time anisotropy with an N-S fast direction and 0.4-1 s delay times is observed to the region about 50 km south of the IYS. This weak anisotropy correlates with the flat part of the subducting Indian lithosphere and could be caused by northward asthenospheric flow and shear at the base of the Indian lithosphere. The null measurements in the vicinity of the IYS are most likely the result of a vertical asthenos pheric flow, the comer flow induced by the subvertical subduction of the Indian lithosphere just south of the Bangong-Nujiang suture (BNS). Observations of the systematic changes in mantle anisotropy from southern Tibet to the central Tibet provide evidence for different mantle flow fields between the Indian and the Eurasian asthenospheric mantle, which are separated by a subvertical Indian lithosphere at the BNS. Copyright 2008 by the American Geophysical Union.},
author = {Fu, Yuanyuan V. and Chen, Y. John and Li, Aibing and Zhou, Shiyong and Liang, Xiaofeng and Ye, Guoyang and Jin, Ge and Jiang, Mingming and Ning, Jieyuan},
doi = {10.1029/2007GL031753},
file = {:D$\backslash$:/Documents/mendeley/Fu et al/Geophysical Research Letters/Fu et al. - 2008 - Indian mantle corner flow at southern Tibet revealed by shear wave splitting measurements.pdf:pdf},
issn = {00948276},
journal = {Geophys. Res. Lett.},
keywords = {http://dx.doi.org/10.1029/2007GL031753, doi:10.102},
number = {2},
pages = {1--6},
title = {{Indian mantle corner flow at southern Tibet revealed by shear wave splitting measurements}},
volume = {35},
year = {2008}
}
