file = {:D$\backslash$:/Documents/mendeley/Dilip, Pasupuleti, Ramancharla/Cloud Publications International Journal of Advanced Earth Science and Engineering/Dilip, Pasupuleti, Ramancharla - 2014 - Interactions and Seismicity of Indian Tectonic Plate with its Neighboring.pdf:pdf},
journal = {Cloud Publ. Int. J. Adv. Earth Sci. Eng.},
keywords = {Crustal Thickness,Earthquake Data,Interaction Lengths,Plate Interaction Types},
number = {1},
pages = {151--170},
title = {{Interactions and Seismicity of Indian Tectonic Plate with its Neighboring Plates: An Overview}},
url = {http://scientific.cloud-journals.com/index.php/IJAESE/article/view/Sci-202},
volume = {3},
year = {2014}
}
@article{Chandel2011,
abstract = {Landslides are short lived and suddenly occurring natural phenomena; it is just a hazard when it occurs in an uninhabited place, however it turns into a disaster causing extraordinary landscape changes and destruction of life and property when it occurs in the vicinity of human habitation. Landslides are particularly common and cause massive damage in tectonically active Himalayas. This work conducts a landslide hazard zonation in western Himalayan district of Kullu in Himachal Pradesh using remote sensing and GIS. The western Himalayan district of Kullu with a location on the southern side of Pirpanjal mountain range, an established history and inherent susceptibility to massive landslides has been chosen for landslide hazard zonation. The satellite imageries of LANDSAT ETM+, IRS P6, ASTER along with Survey of India (SOI) topographical sheets formed the basis for deriving baseline information on various parameters like slope, aspect, relative relief, drainage density, geology/lithology and land use/land cover. The weighted parametric approach was applied to determine degree of susceptibility to landslides. The landslide probability values thus obtained were classified into no risk, very low to moderate, high, and very high to severe landslide hazard risk zones. The results show that over 80 per cent area is liable to high- severe landslide risk and within this about 32 per cent has very high to severe risk.},
author = {Chandel, Vishwa B. S. and Brar, Karanjot Kaur and Chauhan, Yashwant},
file = {:D$\backslash$:/Documents/mendeley/Chandel, Brar, Chauhan/International Journal of Geomatics and Geosciences/Chandel, Brar, Chauhan - 2011 - RS {\&} GIS Based Landslide Hazard Zonation of Mountainous Terrains A Study from Middle Himalayan Kullu Dis.pdf:pdf},
issn = {0976-4380},
journal = {Int. J. Geomatics Geosci.},
keywords = {gis,hazard zonation,landslide,remote sensing},
number = {1},
pages = {121--133},
title = {{RS {\&} GIS Based Landslide Hazard Zonation of Mountainous Terrains A Study from Middle Himalayan Kullu District, Himachal Pradesh, India.}},
volume = {2},
year = {2011}
}
@article{Judycki1992,
abstract = {The non-linear viscoelastic behaviour of asphaltic concrete was tested under creep at bending. Two types of asphaltic concrete were tested: conventional and elastomer-modified mixtures. At 10°C both mixtures exhibited non-linear properties which were more significant for longer times of loading and at greater magnitudes of acting stresses. For short times of loading and for low stresses the non-linear effects diminished. At -10 and -20°C the non-linear effects under creep were not found. A mathematical model of non-linear viscoelasticity is proposed. The model well fits the experimental data for creep of asphaltic concrete at constant loading. {\textcopyright} 1992 RILEM.},
author = {Judycki, J.},
doi = {10.1007/BF02472462},
