file = {:D$\backslash$:/Documents/mendeley/Khattri/Tectonophysics/Khattri - 1987 - Great earthquakes, seismicity gaps and potential for earthquake disaster along the Himalaya plate boundary.pdf:pdf},
issn = {00401951},
