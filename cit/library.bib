@article{Mahesh2012a,
abstract = {We analyze GPS data from 26 sites located on the Indian plate and along its boundary. The large spatial coverage of the Indian plate by these sites and longer data duration helped us in refining the earlier estimates of the Euler pole for the Indian plate rotation. Our analysis suggests that the internal deformation of the Indian plate is very low ({\textless}1-2mm/year) and the entire plate interior region largely behaves as a rigid plate. Specifically, we did not infer any significant difference in motion on sites located north and south of the Narmada Son failed rift region, the most prominent tectonic feature within the Indian plate and a major source of earthquakes. Our analysis also constrains the motion across the Indo-Burmese wedge, Himalayan arc, and Shillong Plateau and Kopili fault in the NE India. {\textcopyright} 2012 International Association for Gondwana Research.},
author = {Mahesh, P. and Catherine, J. K. and Gahalaut, V. K. and Kundu, Bhaskar and Ambikapathy, A. and Bansal, Amit and Premkishore, L. and Narsaiah, M. and Ghavri, Sapna and Chadha, R. K. and Choudhary, Pallabee and Singh, D. K. and Singh, S. K. and Kumar, Subhash and Nagarajan, B. and Bhatt, B. C. and Tiwari, R. P. and Kumar, Arun and Kumar, Ashok and Bhu, Harsh and Kalita, S.},
doi = {10.1016/j.gr.2012.01.011},
file = {:D$\backslash$:/Documents/mendeley/Mahesh et al/Gondwana Research/Mahesh et al. - 2012 - Rigid Indian plate Constraints from GPS measurements.pdf:pdf},
issn = {1342937X},
journal = {Gondwana Res.},
keywords = {Euler pole,Failed rift regions,Global positioning system,India Plate motion,Lithospheric plate interiors},
number = {3-4},
pages = {1068--1072},
publisher = {International Association for Gondwana Research},
title = {{Rigid Indian plate: Constraints from GPS measurements}},
url = {http://dx.doi.org/10.1016/j.gr.2012.01.011},
volume = {22},
year = {2012}
}
@article{Cheng2013,
abstract = {The Dafalias-Manzari (2004) version of SANISAND model, an elastoplastic con-stitutive model for granular liquefiable soils, is implemented as a user-defined model in the plat-form of FLAC3D, a three-dimensional explicit finite difference program using fast Lagrangian analysis of continua for engineering mechanics computation. This is a trial of fully nonlinear analysis method, which contrasts with the commonly used equivalent-linear method, in ge-otechnical earthquake engineering. The simulative capability is validated by comparing the re-sults of simulation with drained and undrained triaxial compression laboratory data over various mean effective pressures and void ratios. Simulations of cyclic stress ratio versus number of cy-cles, back-calculation of strain-compatible shear moduli and damping ratios, free-field nonlinear response, and slope dynamic analysis in terms of effective stresses are presented using the mod-el in FLAC3D. Application of SANISAND Dafalias-Manzari model in FLAC3D (PDF Download Available). Available from: https://www.researchgate.net/publication/283503214{\_}Application{\_}of{\_}SANISAND{\_}Dafalias-Manzari{\_}model{\_}in{\_}FLAC3D [accessed Mar 7, 2016].},
