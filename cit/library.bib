file = {:D$\backslash$:/Documents/mendeley/Wu, Zuo, Lu/Advanced Materials Research/Wu, Zuo, Lu - 2013 - Study on the constitutive model of visco-elasticity-plasticity considering the rheology of rock mass.pdf:pdf},
isbn = {9783037855843},
issn = {10226680},
journal = {Adv. Mater. Res.},
keywords = {Constitutive model,Rheology,Visco-elasticity-plasticity},
