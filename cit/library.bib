journal = {Earthq. Eng. Struct. Dyn.},
keywords = {concrete,concrete model,earthquake analysis,non-linear analysis,plastic damage,stiffness degradation},
number = {9},
pages = {937--956},
title = {{A plastic-damage concrete model for earthquake analysis of dams}},
volume = {27},
year = {1998}
}
