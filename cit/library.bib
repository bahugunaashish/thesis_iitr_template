file = {:D$\backslash$:/Documents/mendeley/Longbir, Arjun/Disaster Advances/Longbir, Arjun - 2017 - Assessment of liquefaction potential of Guwahati city based on geotechnical standard penetration test (SPT) data.pdf:pdf},
issn = {22784543},
journal = {Disaster Adv.},
keywords = {Cyclic resistance ratio,Cyclic stress ratio,LPI,SPT,Soil liquefaction},
number = {12},
pages = {10--21},
title = {{Assessment of liquefaction potential of Guwahati city based on geotechnical standard penetration test (SPT) data}},
