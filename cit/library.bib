file = {:D$\backslash$:/Documents/mendeley/Mitsuhiro et al/Journal of Chemical Information and Modeling/Mitsuhiro et al. - 2002 - Earthquake Processes Physical Modelling, Numerical Simulation and Data Analysis Part II.pdf:pdf},
isbn = {9788578110796},
issn = {1098-6596},
keywords = {icle},
