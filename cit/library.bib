isbn = {9788578110796},
issn = {1098-6596},
journal = {Proc. Indian Acad. Sci. (Earth Planet Sci.)},
keywords = {GPS measurements,deformation strain rate,icle,plate velocity.,south Indian trigon},
mendeley-tags = {GPS measurements,deformation strain rate,plate velocity.,south Indian trigon},
pages = {131--146},
pmid = {25246403},
title = {{Microstrain stability of Peninsular India 1864-1994}},
