@article{Angelier2009a,
abstract = {In Northeast India, three major plates interact along two convergent$\backslash$nboundaries: the Himalayas and the Indo-Burma Ranges, which meet at$\backslash$nthe Assam Syntaxis. To clarify this tectonic interaction and the$\backslash$nunderlying dynamics, we determine the regional seismotectonic stress$\backslash$nfrom the stress inversion of 285 double couple focal mechanism solutions$\backslash$nof earthquakes with an average magnitude of 5. We then compare the$\backslash$nreconstructed stress regimes with the available information about$\backslash$ngeodetically determined relative displacements. North-south compression,$\backslash$nin a direction consistent with India-Eurasia convergence, prevails$\backslash$nin the whole area from the Eastern Himalayas to the Bengal Basin,$\backslash$nthrough the Shillong-Mikir Massif and the Upper Assam Valley. E-W$\backslash$nextension in Tibet is related to this N-S India-Eurasia convergence.$\backslash$nNot only does the major N-S compression affect the outer segments$\backslash$nof the Indo-Burma Ranges, it also extends into the descending slab$\backslash$nof Indian lithosphere below these ranges, although stresses at depth$\backslash$nare controlled by bending of the slab beneath the Burmese arc. The$\backslash$nexistence of widespread N-S compression in the Bengal Basin, far$\backslash$naway from the Himalayan front, is compatible with the previously$\backslash$nproposed convergence between a Shillong-Mikir- Assam Valley block$\backslash$nand the Indian craton. E-W compression inside this block supports$\backslash$nthe hypothesis of a component of eastward extrusion. Stress inversion$\backslash$nof focal mechanism solutions in the Indo-Burma Ranges reveals a complex$\backslash$nstress pattern. The Burmese arc and its underlying lithosphere experience$\backslash$nnearly arc-perpendicular extension with ESE-WNW trends in the northernmost,$\backslash$nNE-trending segment and ENE-WSW trends in the main N-S arc segment.$\backslash$nSuch extensional stress, documented from many arcs, is likely a response$\backslash$nto pull from and bending of the subducting plate. At the same time,$\backslash$nthe Indo-Burma Ranges are under compression as a result of oblique$\backslash$nconvergence between the Sunda and Indian plates. The maximum compressive$\backslash$nstress rotates from NE-SW across the inner and northern arc to E-W$\backslash$nnear the Bengal Basin. This rotation is consistent with the deformation$\backslash$npartitioning reflected in the rotation of relative displacement vectors,$\backslash$nfrom a SSW-directed Sunda-Burma motion to a WSW-directed Burma-India$\backslash$nmotion. As a consequence of this partitioning, the major belt-parallel$\backslash$nfault zones show a variety of movements across the main N-S arc segment,$\backslash$nfrom right-lateral slip in the inner ranges to oblique reverse-dextral$\backslash$nslip in the outer ranges and pure thrusting in the westernmost foreland$\backslash$nbelt.},
author = {Angelier, Jacques and Baruah, Saurabh},
doi = {10.1111/j.1365-246X.2009.04107.x},
file = {:D$\backslash$:/Documents/mendeley/Angelier, Baruah/Geophysical Journal International/Angelier, Baruah - 2009 - Seismotectonics in Northeast India A stress analysis of focal mechanism solutions of earthquakes and its kinem.pdf:pdf},
issn = {0956540X},
