file = {:D$\backslash$:/Documents/mendeley/Zhang, Gao, Pak/Soil Dynamics and Earthquake Engineering/Zhang, Gao, Pak - 2017 - Soil and topographic effects on ground motion of a surficially inhomogeneous semi-cylindrical canyon under obli.pdf:pdf},
issn = {02677261},
journal = {Soil Dyn. Earthq. Eng.},
keywords = {Amplification,Ground motion,Inhomogeneity, Canyon,Site effects,Soil layer,Topography,Wave function expansion, exact solution,Wave propagation and scattering},
number = {May 2016},
