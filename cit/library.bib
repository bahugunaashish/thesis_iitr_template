volume = {20},
year = {2010}
}
@article{Bott1984,
abstract = {The sources of lithospheric stress and their distinctive features are briefly reviewed. It is suggested that there are two main categories of lithospheric stress: renewable stress which persists despite continuing stress relaxation and non-renewable stress which can be dissipated by relief of the initial strain. The two most important types of renewable stress arise from plate boundary forces and from isostatically compensated loads. Non-renewable stress systems include bending stresses, membrane stresses and thermal stresses. An important phenomenon generating large stresses at shallow depth is stress amplification caused by lower lithospheric creep. This applies to renewable stresses but not to the non-renewable type. It is suggested that only renewable stresses contribute significantly to tectonic activity. However, bending and thermal stresses are locally important in subducting lithosphere. {\textcopyright} 1984.},
author = {Bott, M. H.P. and Kusznir, N. J.},
doi = {10.1016/0040-1951(84)90190-2},
