@article{Hristov2018,
abstract = {The study addresses the physical background and modeling of linear viscoelastic response functions and their reasonable relationships to the Caputo-Fabrizio fractional operator via the Prony (Dirichlet series) series decomposition. The problem of interconversion with power-law and exponential (single and multi-term functions) has been discussed. Special attentions have been paid on the Prony series decomposition approach, the related interconversion problems and the expression of the viscoelastic constitutive equations in terms of Caputo-Fabrizio fractional operator.},
