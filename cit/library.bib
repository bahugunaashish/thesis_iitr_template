isbn = {9780976757733},
journal = {Contin. Distinct Elem. Numer. Model. Geomech.},
number = {November},
pages = {09--03},
title = {{Application of SANISAND Dafalias-Manzari model in FLAC 3D}},
url = {http://www.itascacg.com/sites/itascacg.com/files/documents/09-03.pdf},
year = {2013}
