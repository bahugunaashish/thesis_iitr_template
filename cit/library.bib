file = {:D$\backslash$:/Documents/mendeley/Firoj, Bahuguna/Innovative Infrastructure Solutions/Firoj, Bahuguna - 2022 - Evaluation of liquefaction potential using ground response analysis incorporating mohr–coulomb model(2).pdf:pdf},
isbn = {0123456789},
issn = {2364-4184},
journal = {Innov. Infrastruct. Solut.},
keywords = {- spt - ground,potential - matlab -,response analysis - liquefaction,separated by,simplified method},
number = {3},
publisher = {Springer International Publishing},
title = {{Evaluation of liquefaction potential using ground response analysis incorporating mohr–coulomb model}},
url = {https://doi.org/10.1007/s41062-022-00810-2},
volume = {7},
year = {2022}
