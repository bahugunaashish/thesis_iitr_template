journal = {Int. J. Eng. Civ. andTechnology},
keywords = {Clayey Soils,Constitutive Models,Critical State,Drucker-Prager,Mohr-Coulomb},
number = {10},
pages = {104--123},
title = {{Applicability of Mohr-Coulomb {\&} Drucker-Prager Models for Assessment of Undrained Shear Behaviour of Clayey soils}},
url = {www.iaeme.com/Ijciet.asp},
volume = {5},
