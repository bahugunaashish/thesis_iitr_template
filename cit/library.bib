@article{Cheng2018,
abstract = {{\textcopyright} 2018 American Society of Civil Engineers. A modified bounding surface plastic sand model for geotechnical earthquake engineering application is developed to capture essential soil dynamic characteristics. The modification is upon the simple critical state compatible, bounding surface theory based, and fabric-dilatancy related sand plasticity model (DM04 model) introduced by Dafalias and Manzari (2004). By revising some formula without destroying the conciseness of the original DM04 model, the modifications improve the matches between the simulated results and various laboratory or field observations with various initial and loading conditions. The modified model switches all void-ratio related formula, including the elastic modulus, plastic hardening modulus, and critical state, to be relative density related. The modified model embraces an easier calibration procedure in terms of in situ data, such as normalized blow counts instead of experimental data in the laboratory. The modified model retains the general 3D formulation, considering Lode angle effect, and thus can be used for general 3D boundary value problems. The modified model preserves the feature that one set of model constants is used to simulate different responses with different initial relative densities and initial stress states.},
author = {Cheng, Zhao},
doi = {10.1061/9780784481479.004},
file = {:D$\backslash$:/Documents/mendeley/Cheng/Geotechnical Special Publication/Cheng - 2018 - A Practical 3D Bounding Surface Plastic Sand Model for Geotechnical Earthquake Engineering Application.pdf:pdf},
issn = {08950563},
journal = {Geotech. Spec. Publ.},
number = {GSP 292},
pages = {37--47},
title = {{A Practical 3D Bounding Surface Plastic Sand Model for Geotechnical Earthquake Engineering Application}},
volume = {2018-June},
year = {2018}
}
