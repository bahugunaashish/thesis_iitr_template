doi = {10.1016/j.gr.2005.10.006},
file = {:D$\backslash$:/Documents/mendeley/Pathak, Ravi Kumar, Sarkar/Gondwana Research/Pathak, Ravi Kumar, Sarkar - 2006 - Seismic structure of Sri Lanka using receiver function analysis A comparison with other high-grade G.pdf:pdf},
issn = {1342937X},
journal = {Gondwana Res.},
