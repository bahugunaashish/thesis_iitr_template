doi = {10.1144/SP483.16},
file = {:D$\backslash$:/Documents/mendeley/Bilham/Geological Society Special Publication/Bilham - 2019 - Himalayan earthquakes A review of historical seismicity and early 21st century slip potential.pdf:pdf},
