issn = {00401951},
journal = {Tectonophysics},
keywords = {Aravalli-Delhi Fold Belt,Collision signatures,Hindou Group,Mangalwar Complex,Sandmata Complex,Thrust fault},
number = {1-4},
pages = {31--33},
publisher = {Elsevier Science B.V. All rights reserved},
title = {{Structure and tectonics of the Proterozoic Aravalli-Delhi Fold Belt in northwestern India from deep seismic reflection studies}},
